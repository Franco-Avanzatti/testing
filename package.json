{
  "name": "testing",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./index.js --mode=prod",
    "dev": "node --watch ./index.js",
    "test": "node ./index.js --mode=test",
    "artillery-pocos": "artillery quick --count 500 --num 2 \"http://localhost:8080/api/sumar/pocos\" -o ./test/artillery/pocos.json",
    "artillery-muchos": "artillery quick --count 500 --num 2 \"http://localhost:8080/api/sumar/muchos\" -o ./test/artillery/muchos.json",
    "artillery-flujo": "artillery run ./test/artillery/flow/config.yaml -o ./test/artillery/flow/flujo.json",
    "artillery-reporte": "artillery report ./test/artillery/flow/flujo.json",
    "mocha": "mocha --exit ./test/mocha",
    "chai": "mocha --exit ./test/chai",
    "super": "cross-env MODE=test mocha --exit ./test/supertest",
    "build-image": "docker build -t testing-node .",
    "build-image-cluster": "docker build -t testing-node-cluster .",
    "build-image-test": "docker build -t testing-node-test .",
    "build-image-clean": "docker build -t testing-node-clean .",
    "build-image-complete": "docker build -t testing-node-complete ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "artillery": "^2.0.23",
    "artillery-plugin-metrics-by-endpoint": "^1.17.0",
    "bcrypt": "^5.1.1",
    "calculator-70435": "^2.2.2",
    "commander": "^13.1.0",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-compression": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.1",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "mocha": "^11.6.0",
    "supertest": "^7.1.1"
  }
}
